{
  "name": "vlad1/psr-framework",
  "description": "My PSR Framework",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladlen Schetinin",
      "email": "vvschetinin@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.1.0",
    "aura/router": "^3.2",
    "laminas/laminas-diactoros": "^2.25",
    "laminas/laminas-httphandlerrunner": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Framework\\": "src/Framework",
      "App\\": "src/App",
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "roave/security-advisories": "dev-master"
  }, 
  "scripts": {
    "serve": "",
    "test": "phpunit --colors=always"
  }
}
